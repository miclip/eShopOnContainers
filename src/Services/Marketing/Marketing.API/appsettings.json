  "EventBusRetryCount": 5,
  "spring": {
    "application": {
      "name": "marketingapi"
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  }
