{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "MongoConnectionString": "mongodb://nosql.data",
  "MongoDatabase": "MarketingDb",
  "IdentityUrl": "https://identityapi.cfapps.io",
  "PicBaseUrl": "https://marketingapi.cfapps.io/api/v1/campaigns/[0]/pic/",
  "AzureServiceBusEnabled": false,
  "SubscriptionClientName": "Marketing",
